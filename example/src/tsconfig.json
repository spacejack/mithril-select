{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "suppressImplicitAnyIndexErrors": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "noLib": false,
    "declaration": false,
    "sourceMap": false,
    "strictNullChecks": true,
    "noImplicitThis": true
  },
  "files": ["main.ts"],
  "compileOnSave": false,
  "buildOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}
